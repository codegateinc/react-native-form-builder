{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": false,
    "strict": true,
    "target": "esnext",
    "baseUrl": "src",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "outDir": "./dist"
  },
  "exclude": ["node_modules"]
}
